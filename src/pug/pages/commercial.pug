extends ../layout/main
block variable
  -
    var title_page = 'Комерційна нерухомість'
    var id_page = 'commercial'
    var class_page = 'commercial'
    // var breadcrumbsBtn = 'filter'
    var breadcrumbsSubitem = [
      //- {
      //-   label: 'новини та акції',
      //-   link: pagesLink[0].documents
      //- }
    ]
block meta
  //- title= 'Documents'
  title= title_page
  meta(name='description', content='')
block head
    link(rel='stylesheet', href="./assets/styles/main.min.css")
block scripts
  script(defer src='./assets/scripts/vendors.bundle.js')
  script(defer src='./assets/scripts/index.bundle.js')
  script(defer src='./assets/scripts/libs.js')
  script(defer src='./assets/scripts/header.js')
  script(defer src='./assets/scripts/scroll-down.js')
  script(defer src='./assets/scripts/footer.js')
block arrow 
  +svg('scroll-down').js-scroll-down 
block content
  include ../includes/breadcrumbs.pug
  section.documents-content
    .documents-sticker
      h5 Commercial 
    h1.documents-title(data-span-entry1) Комерційна нерухомість 
    .post-service
      .post-service__title(data-span-entry1)
        h3 Комерційні приміщення в Житловому Комплексі Park Land, що по вул. Легоцького – відмінний стартовий майданчик для успіху Вашого бізнесу:
      .post-service__content
        .post-service__img
          img(data-paralax src="./assets/images/commercial.jpg", alt="")
        ul.post-service__list
          li.post-service__item 
            .post-service__item--number 1
            .post-service__item--text Вдале місцерозташування в оновленому, інвестиційно привабливому районі
          li.post-service__item 
            .post-service__item--number 2
            .post-service__item--text Продумані та функціональні приміщення різних площ під різні потреби
          li.post-service__item 
            .post-service__item--number 3
            .post-service__item--text Фасадні вітрини з панорамним склінням
          li.post-service__item 
            .post-service__item--number 4
            .post-service__item--text Понад 1500 потенційних клієнтів 
          li.post-service__item 
            .post-service__item--number 5
            .post-service__item--text Інші поточні питання, пов'язані із функціонуванням комплексу
      

      .commercial-link.contacts__link
        .commercial-link__text(data-span-entry1) жк park land - Ідеальне місце для бізнесу
        a(href='#').realty__link
          span.realty__text обрати комерцію

block header
  include ../includes/header.pug
  
block footer
  include ../includes/footer.pug



